        }
        span {
            position: absolute;
            bottom: -15px;
            left: 6px;
            font-size: 24px;
            display: none;
        }
        #text-span-black {
            display: unset;
            color: white;
        }
        #text-span-white {
            color: white;
        }
        #text-span-gray {
            color: gray;
        }
        #text-span-blue {
            color: #1c8dff;
        }
        #text-span-green {
            color: #00d848;
        }
        #text-span-yellow {
            color: yellow;
        }
        #text-span-pink {
            color: #ff5cb0;
        }
        #text-span-red {
            color: red;
        }
        a {
            color: white;
            text-decoration: none;
        }
        .text-list {
            margin: 0;
            padding: 0 35% 0 0;
            display: flex;
            justify-content: space-between;
            list-style: none;
        }
        .text-list-li {
            width: 27px;
            height: 27px;
            border: none;
            border-radius: 50%;
            display: grid;
            place-content: center;
            position: relative;
        }
        .text-link {
            height: 100%;
            width: 100%;
            line-height: 30px;
            font-size: 18px;
            color: transparent;
            border-radius: 50%;
        }
        .black {
            background-color: black;
        }
        .white {
            background-color: white;
        }
        .gray {
            background-color: grey;
        }
        .blue {
            background-color: #3399ff;
        }
        .green {
            background-color: #00d848;
        }
        .yellow {
            background-color: #fcff32;
        }
        .pink {
            background-color: #ff8ac7;
        }
        .red {
            background-color: red;
        }



        

        svg {
            width: 100%;
            transition: filter 300ms ease-in-out;
        }
        .black-color {
            filter: none;
        }
        .white-color {
            filter: invert(100%) sepia(100%) saturate(0%) hue-rotate(288deg) brightness(102%) contrast(102%);
        }
        .gray-color {
            filter: invert(60%) sepia(12%) saturate(100%) hue-rotate(83deg) brightness(120%) contrast(90%);
        }
        .blue-color {
            filter: invert(70%) sepia(100%) saturate(4648%) hue-rotate(183deg) brightness(102%) contrast(250%);
        }
        .green-color {
            filter: invert(65%) sepia(100%) saturate(3000%) hue-rotate(100deg) brightness(102%) contrast(100%);
        }
        .yellow-color {
            filter: invert(75%) sepia(100%) saturate(2000%) hue-rotate(0deg) brightness(102%) contrast(200%);
        }
        .pink-color {
            filter: invert(80%) sepia(18%) saturate(3226%) hue-rotate(298deg) brightness(107%) contrast(101%);
        }
        .red-color {
            filter: invert(28%) sepia(100%) saturate(1000%) hue-rotate(360deg) brightness(90%) contrast(250%);
        }





        h1 {
            font-size: 7.5em;
            margin: 15px 0px 15px 10px;
            font-weight: bold;
        }
        h2 {
            font-weight: bold;
        }
        h3 {
            color: rgb(219, 219, 219);
        }
        h4 {
            color: rgb(202, 202, 202);
        }
        h5 {
            color: rgb(180, 180, 180);
        }
        h6 {
            color: rgb(158, 158, 158);
            font-size: 12px;
            margin: 20px 0 30px 0;
        }
        section {
            display: grid;
            grid-template-columns: 1fr 1fr;
            user-select: none;
        }
        .text {
            max-width: 530px;
        }
        header {
            width: 100%;
            height: 4em;
            background-color: #222222;
            border-bottom: 1px solid #383838;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-pack: center;
            -ms-flex-pack: center;
            justify-content: center;
            -ms-flex-line-pack: center;
            align-content: center;
            overflow: hidden;
            position: fixed;
            top: 0;
            z-index: 10;
        }
        header .header {
            width: 95%;
            max-width: 1200px;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-pack: justify;
            -ms-flex-pack: justify;
            justify-content: space-between;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
        }
        header .header .header-icon a {
            font-weight: bold;
            font-size: 1.4em;
        }
        header .header .header-icon a img {
            width: 23px;
        }
        header .header .header-icon a:hover {
            color: #3399ff;
            -webkit-transition: color 200ms ease;
            transition: color 200ms ease;
        }
        header .header .header-nav {
            width: 50%;
            -webkit-transition: width 500ms ease;
            transition: width 500ms ease;
            min-width: 205px;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-pack: justify;
            -ms-flex-pack: justify;
            justify-content: space-between;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
        }
        header .header .header-nav .header-link {
            background-color: white;
            border-radius: 5px;
            font-family: arial;
            color: black;
            font-weight: 600;
            letter-spacing: 1px;
            font-size: 20px;
            line-height: 24px;
            padding: 5px 14px;
        }
        header .header .header-nav .header-link:hover {
            background-color: #3399ff;
            -webkit-transition: background 300ms ease;
            transition: background 300ms ease;
        }
        header .header .header-nav .header-focus {
            background-color: #3399ff;
        }
        header .header .header-nav .header-link {
            --ripple-color: rgba(0, 0, 0, 0.3);
            --ripple-duration: 1s;
            --ripple-fuzz: 1px;
            border-color: transparent !important;
        }
        header .header .header-nav .header-link:focus:not(:disabled):not(.disabled),
        header .header .header-nav .header-link:active:not(:disabled):not(.disabled),
