            }
            33.1% {
                background-size: 0% 0%, 100% 1000%;
            }
            100% {
                background-size: 0% 0%, 100% 1000%;
                background-position: 0% 0%, 0% 100%;
            }
        }
        header .header .header-nav .header-svg {
            display: none;
            padding: 0;
            width: 31px;
        }
        header .header .header-nav .header-svg svg {
            transition: none;
            margin-top: 3px;
            width: 30px;
            -webkit-filter: invert(100%) sepia(100%) saturate(0%) hue-rotate(288deg) brightness(102%) contrast(102%);
